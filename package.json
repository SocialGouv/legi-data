{
  "name": "@socialgouv/legi-data",
  "version": "2.130.0",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "homepage": "https://github.com/SocialGouv/legi-data",
  "repository": "https://github.com/SocialGouv/legi-data.git",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "license": "Apache-2.0",
  "scripts": {
    "prebuild": "rimraf bin lib",
    "build": "tsc -p tsconfig.lib.json && mkdir lib && mv ./bin/src/* ./lib",
    "data:fetch": "node -r dotenv/config --loader ts-node/esm --experimental-specifier-resolution=node ./scripts/fetch.ts dotenv_config_path=./.env",
    "data:list": "node --loader ts-node/esm --experimental-specifier-resolution=node ./scripts/list.ts",
    "data:match": "node --loader ts-node/esm --experimental-specifier-resolution=node ./scripts/match.ts",
    "data:update": "yarn data:fetch && yarn data:match && yarn data:list",
    "lint:all": "eslint .",
    "lint": "eslint src/**/*.ts scripts/**/*.ts",
    "setup": "node --loader ts-node/esm --experimental-specifier-resolution=node ./scripts/setup.ts",
    "test:watch": "yarn test --watch",
    "test": "jest",
    "typecheck": "tsc --noEmit && tsc -p tsconfig.lib.json --noEmit"
  },
  "dependencies": {
    "unist-util-find": "^1.0.2",
    "unist-util-parents": "^1.0.3"
  },
  "devDependencies": {
    "@socialgouv/dila-api-client": "^1.2.1",
    "@socialgouv/eslint-config-recommended": "^1.107.0",
    "@socialgouv/eslint-config-typescript": "^1.93.0",
    "@types/jest": "^27.0.3",
    "@types/node": "^17.0.21",
    "@types/npmlog": "^4.1.4",
    "debug": "^4.3.3",
    "dotenv": "^16.0.0",
    "eslint": "^7.22.0",
    "eslint-plugin-sort-requires": "^2.1.0",
    "esm": "^3.2.25",
    "husky": "^7.0.4",
    "jest": "^27.4.3",
    "npmlog": "^6.0.0",
    "p-pipe": "4.0.0",
    "p-queue": "^7.2.0",
    "p-retry": "^5.0.0",
    "prettier": "^2.5.1",
    "prompt": "^1.2.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.7.0",
    "tslog": "^3.3.2",
    "typescript": "^4.5.3",
    "unist-util-flat-filter": "^2.0.0"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn lint:all && yarn typecheck && yarn test"
    }
  }
}
