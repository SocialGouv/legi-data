{
  "name": "@socialgouv/legi-data",
  "version": "1.1.23",
  "homepage": "https://github.com/SocialGouv/legi-data",
  "repository": "https://github.com/SocialGouv/legi-data.git",
  "main": "./src/index.js",
  "types": "./src/index.d.ts",
  "license": "Apache-2.0",
  "files": [
    "data",
    "src"
  ],
  "scripts": {
    "data:fetch": "node -r dotenv/config -r esm ./scripts/fetch.js dotenv_config_path=./.env",
    "data:list": "node -r esm ./scripts/list.js",
    "data:match": "node -r esm ./scripts/match.js",
    "data:update": "yarn data:fetch && yarn data:match && yarn data:list",
    "lint": "eslint src/**/*.js",
    "setup": "node -r esm ./scripts/setup.js",
    "test": "yarn test:lint && yarn test:type && yarn test:unit --silent",
    "test:lint": "eslint .",
    "test:type": "tsc",
    "test:unit": "jest",
    "test:watch": "yarn test:unit --watch"
  },
  "dependencies": {
    "unist-util-find": "1.0.1",
    "unist-util-parents": "1.0.3"
  },
  "devDependencies": {
    "@babel/preset-env": "7.10.3",
    "@socialgouv/dila-api-client": "1.1.1",
    "@socialgouv/eslint-config-strict": "0.33.0",
    "@types/jest": "26.0.0",
    "@types/node": "14.0.13",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.0.1",
    "debug": "4.1.1",
    "dotenv": "8.2.0",
    "eslint": "7.3.0",
    "eslint-plugin-sort-requires": "2.1.0",
    "esm": "3.2.25",
    "husky": "4.2.5",
    "jest": "26.0.1",
    "npmlog": "4.1.2",
    "p-pipe": "3.1.0",
    "p-queue": "6.4.0",
    "p-retry": "4.2.0",
    "prettier": "2.0.5",
    "prompt": "1.0.0",
    "semantic-release": "17.1.0",
    "typescript": "3.9.5",
    "unist-util-flat-filter": "1.0.0"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn test"
    }
  },
  "release": {
    "branches": [
      "master"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  }
}
